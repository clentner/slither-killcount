{
    "manifest_version": 2,
    "name": "Slither Kill Count",
    "version": "1.0",
    "description": "Displays a kill count for slither.io",
    "permissions": ["http://slither.io/*", "activeTab"],
    "content_scripts" : [{
        "matches" : [ "http://slither.io/*"],
        "js" : ["kills.js"]
    }],
    "icons": {
        "128": "icon_128.png"
    }
}